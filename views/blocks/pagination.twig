{% block pagination %}
    <nav aria-label="Page navigation">
        <ul class="pagination">
            {% if page > 1 %}
                <li>
                    <a href="/main/index/?do={{ s_uri }}&page={{ page - 1 }}" aria-label="Previous">
                        <span aria-hidden="true">&laquo;</span>
                    </a>
                </li>
            {% endif %}
            {% for i in 1..count_pages %}
                <li class="{% if i==page %} active{% endif %}"><a href="/main/index/?do={{ s_uri }}&page={{ i }}">{{ i }}</a></li>

            {% endfor %}
            {% if page < count_pages %}
                <li>
                    <a href="/main/index/?do={{ s_uri }}&page={{ page + 1 }}" aria-label="Next">
                        <span aria-hidden="true">&raquo;</span>
                    </a>
                </li>
            {% endif %}
        </ul>
    </nav>
{% endblock %}
